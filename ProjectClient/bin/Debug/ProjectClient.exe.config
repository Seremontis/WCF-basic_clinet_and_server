<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ISimpleInfo" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_ISimpleInfo" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:2333/" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ISimpleInfo" contract="ServerProject.ISimpleInfo"
                name="BasicHttpBinding_ISimpleInfo" />
            <endpoint address="http://localhost:2333/wsh" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_ISimpleInfo" contract="ServerProject.ISimpleInfo"
                name="WSHttpBinding_ISimpleInfo">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>